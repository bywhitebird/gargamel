{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "extends": "@whitebird/tsconfig/node.json",
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "ignoreDeprecations": "5.0"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
